aggiungi MATE

#programma
[
	crea num1 a MATE.random di 0,10
	crea num2 a MATE.random di 0,10
	crea num3 a MATE.random di 0,10
	
	se num1 maggiore num2
		se num1 maggiore num3
			schermo "Il massimo è "+num1
	    altrimenti
			schermo "Il massimo è "+num3
	altrimenti
		se num2 maggiore num3
			schermo "Il massimo è "+num2
		altrimenti
			schermo "Il massimo è "+num3
			
	ripeti
		cambia num2 a 1
	quando num2 maggiore di 6
	cambia num2 a 1
	ritorna num2
]